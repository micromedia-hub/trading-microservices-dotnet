{
  "ConnectionStrings": {
    "PortfolioDb": "Host=host.docker.internal;Port=5432;Database=TradingMicroservices.Portfolio;Username=postgres;Password=admin"
  },
  "RabbitMQ": {
    "Host": "host.docker.internal",
    "VirtualHost": "/",
    "User": "guest",
    "Pass": "guest"
  },
  "Processing": {
    "PriceUpdated": {
      "UseSqlUpsert": true,
      "ConcurrencyMode": "Parallel",
      "Partitions": 8,
      "RetryCount": 3,
      "RetryIntervalMilliseconds": 250
    }
  },
  "Jwt": {
    "Audience": "trading-api",
    "RequireHttps": false,
    "SigningKey": "super-secret-test-key-change-me-32+chars"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  }
}
